{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-conditional-assignment": false,
    "curly": false,
    "member-ordering": false,
    "ordered-imports": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "radix": false,
    "member-access": false,
    "interface-name" : false
  }
}
